<tables xmlns="http://labkey.org/data/xml">
  <table tableName="country" tableDbType="TABLE">
    <columns>
      <column columnName="code"/>
      <column columnName="country"/>
    </columns>
  </table>
  <table tableName="tomatoes" tableDbType="TABLE">
    <columns>
      <column columnName="id"/>
      <column columnName="image"/>
      <column columnName="rating"/>
      <column columnName="meter"/>
      <column columnName="reviews"/>
      <column columnName="fresh"/>
      <column columnName="rotten"/>
      <column columnName="consensus"/>
      <column columnName="usermeter"/>
      <column columnName="userrating"/>
      <column columnName="userreviews"/>
      <column columnName="dvd"/>
      <column columnName="boxoffice"/>
      <column columnName="production"/>
      <column columnName="website"/>
      <column columnName="lastupdated"/>
      <column columnName="container"/>
      <column columnName="created"/>
      <column columnName="createdby"/>
      <column columnName="modified"/>
      <column columnName="modifiedby"/>
      <column columnName="ditransformrunid"/>
      <column columnName="dimodified"/>
    </columns>
  </table>
  <table tableName="reviews" tableDbType="TABLE">
    <columns>
      <column columnName="ReviewID"/>
      <column columnName="TitleID"/>
      <column columnName="Rating"/>
      <column columnName="Review"/>
      <column columnName="container"/>
      <column columnName="created"/>
      <column columnName="createdby"/>
      <column columnName="modified"/>
      <column columnName="modifiedby"/>
      <column columnName="ditransformrunid"/>
      <column columnName="dimodified"/>
    </columns>
  </table>
  <table tableName="genre" tableDbType="TABLE">
    <columns>
      <column columnName="genre"/>
    </columns>
  </table>
  <table tableName="titles" tableDbType="TABLE">
    <titleColumn>title</titleColumn>
    <tableUrl>/movies/detail.view?id=${id}</tableUrl>
    <updateUrl></updateUrl>
    <insertUrl></insertUrl>
    <columns>
      <column columnName="id">
        <url>/movies/detail.view?id=${id}</url>
      </column>
      <column columnName="imdbid">
        <url>http://www.imdb.com/title/${imdbid}/</url>
      </column>
      <column columnName="Title"/>
      <column columnName="Year"/>
      <column columnName="Rating"/>
      <column columnName="Runtime"/>
      <column columnName="Genre"/>
      <column columnName="Released">
        <formatString>d MMMM yyyy</formatString>
      </column>
      <column columnName="Director"/>
      <column columnName="Writer"/>
      <column columnName="Cast"/>
      <column columnName="Metacritic"/>
      <column columnName="imdbRating"/>
      <column columnName="imdbVotes"/>
      <column columnName="Poster"/>
      <column columnName="Plot"/>
      <column columnName="FullPlot"/>
      <column columnName="Language"/>
      <column columnName="Country"/>
      <column columnName="Awards"/>
      <column columnName="container"/>
      <column columnName="created"/>
      <column columnName="createdby"/>
      <column columnName="modified"/>
      <column columnName="modifiedby"/>
      <column columnName="ditransformrunid"/>
      <column columnName="dimodified"/>
    </columns>
  </table>
  <table tableName="episodes" tableDbType="TABLE">
    <columns>
      <column columnName="id"/>
      <column columnName="imdbid"/>
      <column columnName="Title"/>
      <column columnName="Year"/>
      <column columnName="Rating"/>
      <column columnName="Runtime"/>
      <column columnName="Genre"/>
      <column columnName="Released"/>
      <column columnName="Season"/>
      <column columnName="Episode"/>
      <column columnName="SeriesID">
      <fk>   
        <fkColumnName>id</fkColumnName>
        <fkTable>titles</fkTable>
      </fk>
      </column>
      <column columnName="Director"/>
      <column columnName="Writer"/>
      <column columnName="Cast"/>
      <column columnName="Metacritic"/>
      <column columnName="imdbRating"/>
      <column columnName="imdbVotes"/>
      <column columnName="Poster"/>
      <column columnName="Plot"/>
      <column columnName="FullPlot"/>
      <column columnName="Language"/>
      <column columnName="Country"/>
      <column columnName="Awards"/>
      <column columnName="lastupdated"/>
      <column columnName="container"/>
      <column columnName="created"/>
      <column columnName="createdby"/>
      <column columnName="modified"/>
      <column columnName="modifiedby"/>
      <column columnName="ditransformrunid"/>
      <column columnName="dimodified"/>
    </columns>
  </table>
</tables>
